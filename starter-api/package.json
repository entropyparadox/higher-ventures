{
  "name": "starter-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build front-api && nest build admin-api && nest build batch",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:front": "nest start front-api",
    "start:front:dev": "nest start front-api --watch",
    "start:admin": "nest start admin-api",
    "start:admin:dev": "nest start admin-api --watch",
    "start:batch": "nest start batch",
    "start:batch:dev": "nest start batch --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:schema:sync": "NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "test": "yarn run test:schema:sync && jest -i",
    "test:watch": "yarn run test:schema:sync && jest -i --watch",
    "test:front:watch": "yarn run test:schema:sync && jest -i --watch -- apps/front-api",
    "test:admin:watch": "yarn run test:schema:sync && jest -i --watch -- apps/admin-api",
    "test:batch:watch": "yarn run test:schema:sync && jest -i --watch -- apps/batch",
    "test:front:e2e": "yarn run test:schema:sync && jest --config ./apps/front-api/test/jest-e2e.json",
    "test:admin:e2e": "yarn run test:schema:sync && jest --config ./apps/admin-api/test/jest-e2e.json",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "schema:drop": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync",
    "g:resource": "bash tasks/generate/resource.sh",
    "g:resource:js": "node tasks/generate/resource.js",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run:dev": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateDevSeed",
    "seed:run:prod": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -s CreateProdSeed",
    "seed:reset:dev": "yarn run schema:drop && yarn run schema:sync && yarn run seed:run:dev",
    "seed:reset:prod": "yarn run schema:drop && yarn run schema:sync && yarn run seed:run:prod"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.1",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.1.1",
    "@nestjs/throttler": "^2.0.1",
    "@nestjs/typeorm": "~8.0.4",
    "@types/pluralize": "^0.0.29",
    "aws-sdk": "^2.1147.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "dotenv": "^16.0.0",
    "iconv-lite": "^0.6.3",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^2.1.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "mysql2": "^2.3.3",
    "nanoid": "^3.1.31",
    "nest-winston": "^1.7.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "pluralize": "^8.0.0",
    "qs": "^6.10.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.32",
    "typeorm-seeding": "^1.6.1",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "winston": "^3.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/faker": "^6.6.9",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.7.4"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "apps/(.*)": "<rootDir>/apps/$1",
      "env.config": "<rootDir>/env.config.ts"
    },
    "roots": ["<rootDir>/apps/"],
    "restoreMocks": true,
    "setupFilesAfterEnv": ["./apps/infrastructure/testing/jest.setup.js"]
  },
  "volta": {
    "node": "16.14.2",
    "npm": "8.5.5",
    "yarn": "1.22.17"
  }
}
